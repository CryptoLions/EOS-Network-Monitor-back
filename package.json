{
  "name": "eos-be-monitor",
  "version": "1.0.1",
  "description": "CryptoLions.io",
  "scripts": {
    "start-app": "node --max-old-space-size=8192 ./src/app",
    "start-routine": "node --max-old-space-size=8192 ./src/routines/index",
    "updateTransactionsWithPM2": "pm2 start ./src/utils/updateTransactions/start.js --name updateTransactions",
    "updateTransactions": "node ./src/utils/updateTransactions/start.js",
    "updateTpsAps": "node ./src/utils/updateTpsAps/start.js",
    "updateTotalTransactionsCount": "node ./src/utils/updateTotalTransactionsCount/start.js",
    "cleanAccountCollectionFromMentionedIn": "node ./src/utils/cleanAccountCollectionFromMentionedIn/start.js",
    "startAPP": "concurrently --names \"APP\" \"npm run start-app\"",
    "startROUTINE": "concurrently --names \"ROUTINE\" \"npm run start-routine\"",
    "pm2-start": "pm2 start ecosystem.config.js --env production",
    "migrateTransactionToAccount": "pm2 start ./src/migrations/fillTransactionToAccountCollection.js --name tansactionToAccountMigration",
    "collectAccountTokens": "pm2 start ./src/utils/collectAccountTokens/index.js --name collectAccountTokens",
    "updateTransactionToAccount": "node ./src/utils/updateTransactionToAccountCollection/start.js",
    "checkProducedBlocks": "node ./src/utils/checkProducedBlocks/start.js",
    "fillBlockGaps": "node ./src/utils/fillBlockGaps/start.js",
    "test": "./node_modules/mocha/bin/mocha --timeout 10000 tests/**/*test.js"
  },
  "dependencies": {
    "appmetrics-dash": "^4.0.0",
    "assert": "^1.4.1",
    "async": "^2.6.1",
    "body-parser": "^1.18.3",
    "bugsnag": "^2.4.3",
    "chai": "^4.2.0",
    "compression": "^1.7.2",
    "concurrently": "^3.6.0",
    "config": "^1.30.0",
    "cors": "^2.8.5",
    "cron": "^1.6.0",
    "eosjs": "^15.0.3",
    "eosjs-api": "^7.0.4",
    "express": "^4.16.4",
    "hoek": "^5.0.3",
    "lodash": "^4.17.11",
    "mocha": "^5.2.0",
    "moment": "^2.24.0",
    "mongodb": "^3.1.13",
    "mongoose": "^5.4.6",
    "path": "^0.12.7",
    "pidusage": "^2.0.17",
    "request": "^2.85.0",
    "request-promise": "^4.2.2",
    "request-promise-native": "^1.0.5",
    "sharp": "^0.21.3",
    "slack": "^11.0.1",
    "sleep": "^5.2.4",
    "socket.io": "2.1.1",
    "url-parse": "^1.4.4",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "prettier": "^1.16.1"
  }
}
